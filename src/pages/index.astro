---
export const prerender = true;

import BaseLayout from "@layouts/base.astro";
import BrainDumpAd from "@components/BrainDumpAd.astro";
import HomeMast from "@components/HomeMast.astro";
import LatestPosts from "@components/LatestPosts.astro";
import Proof from "@components/Proof.astro";

const title = "Michael Jolley";
const subhead = "Bald. Bearded. Builder.";
const description =
  "Welcome to the home of Michael Jolley, also known as the bald, bearded, builder. Here you'll find all Michael's content curated for easier viewing.";
const permalink = `https://${import.meta.env.HOST}/`;
---

<BaseLayout {title} {description} {permalink} {subhead}>
	<HomeMast/>
	<Proof/>
	<LatestPosts/>
	<BrainDumpAd/>
</BaseLayout>
