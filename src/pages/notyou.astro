---
export const prerender = false;
import { authData } from "../lib/auth";

const title = "You Get Nothing!";
const description =
	"Chris, you know I'm not going to let this happen.";
const permalink = `https://${import.meta.env.HOST}/`;

const user = await authData(Astro.cookies);

---

<html>
	<head>
		<!-- Global Metadata -->
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		
		<link rel="icon" type="image/x-icon" href="/images/favicon/favicon.ico" />
		<link
			rel="icon"
			type="image/png"
			href="/images/favicon/favicon-16x16.png"
			sizes="16x16"
		/>
		<link
			rel="icon"
			type="image/png"
			href="/images/favicon/favicon-32x32.png"
			sizes="32x32"
		/>
		<link
			rel="apple-touch-icon"
			href="/images/favicon/apple-touch-icon.png"
			sizes="180x180"
		/>
		
		<!-- Primary Meta Tags -->
		<title>{title}</title>
		<meta name="title" content={title} />
		<meta name="description" content={description} />
	</head>
	<body>
		
	</body>

</html>

<style>

body {
	margin: 0;
	background-color: #9CCB41;
	background-image: url('/images/chris.png');
	background-size: cover;
	background-repeat: no-repeat;
	height: 100%;
}


</style>
