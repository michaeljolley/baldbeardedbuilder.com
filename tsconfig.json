{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@styles/*": ["src/styles/*"],
			"@scripts/*": ["src/scripts/*"],
			"@lib/*": ["src/lib/*"],
			"@types/*": ["netlify/edge-functions/types/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	}
}
